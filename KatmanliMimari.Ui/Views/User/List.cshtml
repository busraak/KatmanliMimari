@model IEnumerable<KatmanliMimari.Entity.Users.AspNetUser>

@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>List</h1>

<table class="table">
    <thead>
        <tr>
                <th>
                Id
            </th>
            <th>
                UserName
            </th>
             <th>
                Roles
            </th>
      
       
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
             <td>
                @item.Id
            </td>
            <td>
                @item.UserName
            </td>
              
                @foreach (var x in item.Roles)
                {
                    string roles = "";
                    roles += x.Name + " ";
                    <td>@roles</td>
                }
            
        
   
            <td>
              <a class="btn btn-success" asp-action="Create" asp-route-id="@item.Id">Edit</a> 
                
            </td>
        </tr>
}
    </tbody>
</table>
